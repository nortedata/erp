    },
    "COD_ITEM": {
        "type": "string",
        "regex": "^.{0,60}$",
        "required": false,
        "info": "C\u00f3digo do item (campo 02 do Registro 0200) ",
        "format": ""
    },
    "COD_MOD": {
        "type": "string",
        "regex": "^.{2}$",
