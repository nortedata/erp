{
    "COD_MOD": {
        "type": "string",
        "regex": "^(2E|13|14|15|16)$",
        "required": false,
        "info": "C\u00f3digo do modelo do documento fiscal, conforme a Tabela 4.1.1 ",
        "format": ""
    },
    "ECF_MOD": {
        "type": "string",
        "regex": "^.{0,20}$",
        "required": false,
        "info": "Modelo do equipamento ",
        "format": ""
    },
    "ECF_FAB": {
        "type": "string",
