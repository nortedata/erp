        "format": ""
    },
    "DT_DOC": {
        "type": "string",
        "regex": "^(0[1-9]|[1-2][0-9]|31(?!(?:0[2469]|11))|30(?!02))(0[1-9]|1[0-2])([12]\\d{3})$",
        "required": false,
        "info": "Data do movimento a que se refere a Redu\u00e7\u00e3o Z ",
        "format": ""
    },
    "CRO": {
        "type": "numeric",
